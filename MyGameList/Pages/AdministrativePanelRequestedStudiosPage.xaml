        <ScrollViewer HorizontalAlignment="Left" Height="663" Margin="0,57,0,0" VerticalAlignment="Top" Width="1200">
            <WrapPanel x:Name="RequestedStudiosWrapPanel" Width="1200">
            </WrapPanel>
        </ScrollViewer>
        <Button Click="RequestedStudiosButton_Clicked" Cursor="Hand" Background="White" Foreground="#FF0FB200" Content="Requested Studios" HorizontalAlignment="Left" Margin="439,10,0,0" VerticalAlignment="Top"/>
        <Button Click="StudiosButton_Clicked" Cursor="Hand" Foreground="#FF0FB200" Content="Studios" HorizontalAlignment="Left" Margin="622,10,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="0.337,0.011" Background="White"/>
